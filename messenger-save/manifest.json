{
    "manifest_version": 3,
    "name": "messenger-saver",
    "version": "1.0",
    "permissions": ["storage"],    
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
        {
          "matches": ["*://www.messenger.com/*","*://messenger.com/*"],
          "js": ["content.js"]
        }
      ]
  }
  